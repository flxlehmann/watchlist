{
  "version": 2,
  "builds": [
    {
      "src": "api/rooms/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/api/rooms$",
      "dest": "/api/rooms/index.js"
    },
    {
      "src": "^/api/rooms/.*$",
      "dest": "/api/rooms/index.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^(?!/api)(.*)$",
      "dest": "/public/index.html"
    }
  ]
}